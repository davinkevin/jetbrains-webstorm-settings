    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="descr" value="describe('$TITLE$', () =&gt; {&#10;    $END$&#10;});" description="Jasmine Describe Test" toReformat="true" toShortenFQNames="true">
